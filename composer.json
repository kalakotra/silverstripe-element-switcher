{
    "name": "kalakotra/silverstripe-element-switcher",
    "description": "This module provides simplified navigation between elemental elements.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "Elemental",
        "Switch elements"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^6.1",
        "silverstripe/admin": "^3",
        "dnadesign/silverstripe-elemental": "^6",
        "lekoala/silverstripe-cms-actions": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1",
        "phpstan/extension-installer": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Kalakotra\\SilverstripeElementSwitcher\\": "src/",
            "Kalakotra\\SilverstripeElementSwitcher\\Tests\\": "tests/php/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
